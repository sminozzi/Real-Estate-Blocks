{
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
    "version": 2,
    "title": "Red",
    "settings": {
        "color": {
            "palette": [
                {
                    "color": "#FFFFFF",
                    "name": "Base",
                    "slug": "base"
                },
                {
                    "color": "#f6decd",
                    "name": "Base / Two",
                    "slug": "base-2"
                },
                {
                    "color": "#000000",
                    "name": "Contrast",
                    "slug": "contrast"
                },
                {
                    "color": "#F82F58",
                    "name": "Contrast / 2",
                    "slug": "contrast-2"
                },
                {
                    "color": "#3e3e3e",
                    "name": "Top Bar",
                    "slug": "topbar"
                },
                {
                    "color": "#F82F58",
                    "name": "Background Menu",
                    "slug": "backmenu"
                },
                {
                    "color": "#FFFFFF",
                    "name": "Color Menu",
                    "slug": "colormenu"
                }
            ]
        }
    },
    "styles": {
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--contrast)",
                    "text": "var(--wp--preset--color--base)"
                }
            },
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                }
            }
        }
    }
}